{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "vercel_app.py"
    },
    {
      "src": "/test_connection",
      "dest": "vercel_app.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
